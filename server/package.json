{
    "dependencies": {
        "@dotenvx/dotenvx": "^1.51.1",
        "@y/websocket-server": "^0.1.1",
        "cors": "^2.8.5",
        "express": "^5.2.1",
        "find-process": "^2.0.0",
        "firebase-admin": "^13.6.0",
        "fs-extra": "^11.3.2",
        "jsonwebtoken": "^9.0.3",
        "pino": "^9.8.0",
        "pino-pretty": "^13.1.3",
        "uuid": "^11.1.0",
        "ws": "^8.18.1",
        "y-leveldb": "^0.2.0",
        "y-websocket": "^3.0.0",
        "yjs": "^13.6.27",
        "yjs-orderedtree": "^1.0.1-beta.3",
        "zod": "^4.1.13"
    },
    "description": "Authentication server for Fluid Outliner app",
    "devDependencies": {
        "@types/express": "^5.0.6",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.18.1",
        "chai": "^6.2.1",
        "dotenv": "^17.2.0",
        "jest": "^30.0.5",
        "mocha": "^11.7.5",
        "nodemon": "^3.1.11",
        "patch-package": "^8.0.1",
        "sinon": "^21.0.0",
        "supertest": "^7.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "jest": {
        "clearMocks": true,
        "testEnvironment": "node",
        "testMatch": [
            "**/tests/**/*.test.js"
        ]
    },
    "main": "dist/index.js",
    "name": "fluid-outliner-server",
    "scripts": {
        "build": "tsc",
        "create-test-user": "node utils/create-test-user.js",
        "dev": "nodemon log-service.js",
        "start": "node dist/index.js",
        "start:log-service": "node log-service.js",
        "test": "bash ../scripts/run-server-tests.sh",
        "test:emulator-wait": "mocha tests/log-service-emulator-wait.test.js --timeout 10000",
        "postinstall": "patch-package",
        "postbuild": "mkdir -p dist/yjs-orderedtree-cjs && cp src/yjs-orderedtree-cjs/* dist/yjs-orderedtree-cjs/ && sed -i \"s|require(\\\"./yjs-orderedtree-cjs/index.js\\\")|require(\\\"./yjs-orderedtree-cjs\\\")|g\" dist/app-schema.js"
    },
    "version": "1.0.0",
    "resolutions": {
        "yjs": "^13.6.27"
    }
}
