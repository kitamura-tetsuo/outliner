DOTENV_PUBLIC_KEY_DEVELOPMENT="02cb4c24270c32f046217f0e05c88e9dfba6285a2792a5760fb91883ccdd787951"

# .env.development
# 開発環境設定

# サーバー設定
VITE_HOST=0.0.0.0
VITE_PORT=7070

# Firebase設定
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Azure Fluid Relay設定
VITE_AZURE_TENANT_ID=your-tenant-id
VITE_AZURE_FLUID_RELAY_ENDPOINT=https://us.fluidrelay.azure.com

# 接続サービスの選択
# true: Tinyliciousを使用、false: Azureを使用
VITE_USE_TINYLICIOUS=false
# 開発環境でAzureを強制的に使用する
VITE_FORCE_AZURE=true

# 認証設定
VITE_USE_FIREBASE_AUTH=true
VITE_USE_API_AUTH=true

# API設定
VITE_API_BASE_URL=http://192.168.50.13:7071
VITE_API_SERVER_URL=http://192.168.50.13:7071
VITE_AZURE_FUNCTION_URL=http://192.168.50.13:7072

# Tinylicious設定
VITE_TINYLICIOUS_ENDPOINT=http://192.168.50.13:7072
VITE_TINYLICIOUS_PORT=7072
VITE_FLUID_TOKEN_KEY=test-key

# デバッグ用設定
VITE_DEBUG_USER_ID=dev-user
VITE_DEBUG_USER_NAME=Developer

# Fluid Framework Telemetry設定
VITE_DISABLE_FLUID_TELEMETRY=true
