{
  "name": "outliner",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host 0.0.0.0",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest run",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:core": "playwright test --project=core",
    "test:auth": "playwright test --project=auth",
    "test:e2e:debug": "NODE_ENV=test dotenv -e .env.test -- playwright test --debug",
    "test:e2e:ui": "NODE_ENV=test dotenv -e .env.test -- playwright test --ui",
    "test:e2e:codegen": "NODE_ENV=test dotenv -e .env.test -- playwright codegen",
    "test:e2e:xvfb": "NODE_ENV=test dotenv -e .env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" npm run test:e2e"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@fluidframework/test-client-utils": "^1.4.0",
    "@fluidframework/tinylicious-client": "^2.30.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/dotenv": "^6.1.1",
    "@types/uuid": "^10.0.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "tinylicious": "^0.6.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@fluidframework/azure-client": "^2.30.0",
    "@fluidframework/fluid-static": "^2.30.0",
    "@fluidframework/sequence": "^2.30.0",
    "@fluidframework/tree": "^2.30.0",
    "@inlang/paraglide-sveltekit": "^0.15.5",
    "dotenv": "^16.4.7",
    "firebase": "^11.5.0",
    "fluid-framework": "^2.30.0",
    "uuid": "^8.3.2"
  }
}