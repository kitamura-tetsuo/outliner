{
    "name": "outliner",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "npm run paraglide:compile && vite dev",
        "dev:host": "npm run paraglide:compile && vite dev --host 0.0.0.0",
        "dev:localhost": "npm run paraglide:compile && NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- vite dev --host 0.0.0.0 --port 7090",
        "dev:localhost:tinylicious": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- npx tinylicious --port 7092",
        "build": "npm run paraglide:compile && vite build",
        "build:production": "npm run paraglide:compile && dotenvx run --env-file=.env.production -- vite build",
        "preview": "vite preview",
        "preview:host": "vite preview --host 0.0.0.0",
        "prepare": "svelte-kit sync || echo ''",
        "paraglide:compile": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "dprint fmt",
        "lint": "dprint check && eslint .",
        "test:unit": "vitest run --project unit --coverage",
        "test:integration": "vitest run --project integration",
        "test:production": "vitest run --project production",
        "test:production:watch": "vitest --project production",
        "github:test:unit": "vitest run --project unit --coverage --reporter=github-actions --reporter=dot",
        "github:test:integration": "vitest run --project integration --reporter=github-actions --reporter=dot",
        "github:test:production": "vitest run --project production --reporter=github-actions --reporter=dot",
        "test": "NODE_ENV=test dotenvx run --env-file=.env.test -- npm run test:unit && npm run test:integration && npm run test:e2e",
        "test:all": "NODE_ENV=test dotenvx run --env-file=.env.test -- npm run test:unit && npm run test:integration && npm run test:e2e",
        "test:e2e": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=list",
        "github:test:e2e": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=github,line",
        "test:e2e:basic": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test ./e2e/basic --reporter=list",
        "test:e2e:debug": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=list --debug",
        "test:e2e:ui": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=list --ui",
        "test:e2e:xvfb": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=list",
        "ci:test:e2e": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --reporter=list",
        "ci:test:e2e:ui": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test --ui --reporter=list"
    },
    "devDependencies": {
        "@dotenvx/dotenvx": "^1.48.1",
        "@eslint/compat": "^1.3.1",
        "@eslint/js": "^9.31.0",
        "@fluidframework/test-client-utils": "^1.4.0",
        "@fluidframework/tinylicious-client": "^2.51.0",
        "@inlang/message-lint-rule-empty-pattern": "^1.4.8",
        "@inlang/message-lint-rule-identical-pattern": "^1.5.9",
        "@inlang/message-lint-rule-missing-translation": "^1.4.8",
        "@inlang/message-lint-rule-valid-js-identifier": "^1.0.8",
        "@inlang/message-lint-rule-without-source": "^1.4.8",
        "@inlang/paraglide-js": "2.2.0",
        "@inlang/plugin-m-function-matcher": "^2.0.10",
        "@inlang/plugin-message-format": "^4.0.0",
        "@playwright/test": "^1.53.0-alpha-2025-05-30",
        "@sveltejs/adapter-auto": "^6.0.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.27.0",
        "@sveltejs/vite-plugin-svelte": "^6.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.15",
        "@tailwindcss/vite": "^4.1.11",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/svelte": "^5.2.4",
        "@types/diff-match-patch": "^1.0.36",
        "@types/dotenv": "^8.2.3",
        "@types/uuid": "^10.0.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^9.2.0",
        "eslint": "^9.32.0",
        "eslint-plugin-svelte": "^3.11.0",
        "fake-indexeddb": "^6.0.1",
        "find-process": "^2.0.0",
        "globals": "^16.3.0",
        "jsdom": "^26.0.0",
        "knip": "^5.62.0",
        "mdsvex": "^0.12.3",
        "patch-package": "^6.5.1",
        "playwright": "^1.54.2",
        "svelte": "^5.37.3",
        "svelte-check": "^4.3.0",
        "svelte-preprocess": "^6.0.3",
        "tailwindcss": "^4.0.0",
        "tinylicious": "^6.0.0",
        "ts-prune": "^0.10.3",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.1",
        "vite": "^7.0.6",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.48.1",
        "@fluidframework/azure-client": "^2.52.0",
        "@fluidframework/fluid-static": "^2.51.0",
        "@fluidframework/sequence": "^2.52.0",
        "@fluidframework/tree": "^2.51.0",
        "diff-match-patch": "^1.0.5",
        "echarts": "^5.6.0",
        "firebase": "^12.1.0",
        "fluid-framework": "2.51.0",
        "jsonwebtoken": "^9.0.2",
        "pino": "^9.8.0",
        "rrule": "^2.8.1",
        "sql.js": "^1.13.0",
        "uuid": "^11.1.0",
        "wx-svelte-grid": "^2.2.0"
    }
}
