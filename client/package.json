    "name": "outliner",
    "private": true,
    "scripts": {
        "build": "npm run paraglide:compile && vite build",
        "build:production": "npm run paraglide:compile && dotenvx run --env-file=.env.production -- vite build",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "ci:test:e2e": "env PATH=\"./node_modules/.bin:$PATH\" bash -lc 'npx playwright test' --",
        "ci:test:e2e:ui": "env PATH=\"./node_modules/.bin:$PATH\" bash -lc 'playwright test --ui' --",
        "dev": "npm run paraglide:compile && vite dev",
        "dev:host": "npm run paraglide:compile && vite dev --host 0.0.0.0",
        "dev:localhost": "npm run paraglide:compile && NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- bash -lc 'E2E_DISABLE_HMR=1 E2E_DISABLE_WATCH=1 vite dev --host 0.0.0.0 --port 7090'",
        "dev:localhost:tinylicious": "NODE_ENV=test TEST_ENV=localhost dotenvx run --env-file=.env.test -- npx tinylicious --port 7092",
        "format": "npx dprint fmt",
        "github:test:e2e": "NODE_OPTIONS='--dns-result-order=ipv4first' NODE_ENV=test TEST_ENV=127.0.0.1 dotenvx run --env-file=.env.test -- env PATH=\"./node_modules/.bin:$PATH\" bash -lc 'playwright test \"$@\"' --",
        "github:test:integration": "vitest run --project integration --reporter=github-actions --reporter=dot",
        "github:test:production": "vitest run --project production --reporter=github-actions --reporter=dot",
        "github:test:unit": "vitest run --project unit --coverage --reporter=github-actions --reporter=dot",
        "lint": "npx dprint fmt && eslint . --max-warnings=-1",
        "lint:diff-lines": "node ../scripts/lint-changed-lines.js",
        "paraglide:compile": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
        "postinstall": "npx --yes patch-package",
        "prepare": "svelte-kit sync || echo ''",
        "preview": "vite preview",
        "preview:host": "vite preview --host 0.0.0.0",
        "coverage:e2e": "NODE_OPTIONS=--max-old-space-size=8192 node scripts/generate-e2e-coverage.js",
        "coverage:merge": "node scripts/merge-coverage.js",
        "coverage:backup": "node scripts/backup-coverage.js",
        "coverage:move-to-backup": "node scripts/move-coverage-to-backup.js",
        "coverage:report": "vitest run --project unit --project integration --coverage && npm run coverage:merge",
        "test": "npm run coverage:move-to-backup && npm run coverage:cleanup && NODE_ENV=test dotenvx run --overload --env-file=.env.test -- vitest run --project unit --project integration --coverage && npm run coverage:backup && npm run test:e2e && npm run coverage:backup && npm run coverage:e2e && npm run coverage:backup && npm run coverage:merge && npm run coverage:backup",
        "test:e2e": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- $PWD/node_modules/.bin/playwright test \"$@\"",
        "test:e2e:basic": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- $PWD/node_modules/.bin/playwright test ./e2e/basic \"$@\"",
        "test:e2e:debug": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- $PWD/node_modules/.bin/playwright test --debug \"$@\"",
        "test:e2e:ui": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- $PWD/node_modules/.bin/playwright test --ui \"$@\"",
        "test:e2e:xvfb": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- $PWD/node_modules/.bin/playwright test \"$@\"",
        "test:e2e:flaky": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- ../scripts/capture-flaky-e2e.js",
        "test:integration": "vitest run --project integration --coverage",
        "test:production": "vitest run --project production",
        "test:production:watch": "vitest --project production",
        "test:unit": "vitest run --project unit --coverage",
        "test:mutation": "NODE_ENV=test dotenvx run --overload --env-file=.env.test -- stryker run --incremental",
        "analyze:polling": "node ../scripts/analyze-polling.mjs",
        "test:polling": "NODE_ENV=test TEST_ENV=localhost dotenvx run --overload --env-file=.env.test -- env PATH=\"./node_modules/.bin:$PATH\" bash -lc 'npx playwright test ./e2e/env/env-polling-analysis-test-removability-a1b2c3d4.spec.ts' --",
        "coverage:cleanup": "node scripts/cleanup-coverage.js"
    },
    "type": "module",
    "version": "0.0.1"
}
