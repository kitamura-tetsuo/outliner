{
    "name": "outliner",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "dev:host": "vite dev --host 0.0.0.0",
        "dev:localhost": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- vite dev --host 0.0.0.0 --port 7090",
        "dev:localhost:tinylicious": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- npx tinylicious --port 7092",
        "build": "vite build",
        "preview": "vite preview",
        "preview:host": "vite preview --host 0.0.0.0",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest run",
        "test": "cross-env NODE_ENV=test dotenv -e .env.test -- npm run test:unit && npm run test:e2e",
        "test:e2e": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- playwright test",
        "test:e2e:debug": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- playwright test --debug",
        "test:e2e:ui": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- playwright test --ui",
        "test:e2e:xvfb": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" playwright test",
        "test:e2e:single": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- playwright test",
        "test:e2e:clipboard": "cross-env NODE_ENV=test TEST_ENV=localhost dotenv -e .env.localhost.test -- playwright test e2e/core/FMT-0004.spec.ts",
        "ci:test:e2e": "playwright test",
        "ci:test:e2e:ui": "playwright test --ui",
        "ci:test:e2e:xvfb": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" npm run ci:test:e2e"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@fluidframework/test-client-utils": "^1.4.0",
        "@fluidframework/tinylicious-client": "^2.30.0",
        "@playwright/test": "^1.52.0",
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.15",
        "@tailwindcss/vite": "^4.0.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/svelte": "^5.2.4",
        "@types/dotenv": "^6.1.1",
        "@types/uuid": "^10.0.0",
        "chokidar-cli": "^2.1.0",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.4.4",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^3.0.0",
        "find-process": "^1.4.7",
        "globals": "^16.0.0",
        "jsdom": "^26.0.0",
        "knip": "^5.51.0",
        "mdsvex": "^0.12.3",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwindcss": "^4.0.0",
        "tinylicious": "^0.6.0",
        "ts-prune": "^0.10.3",
        "typescript": "^5.5.0",
        "typescript-eslint": "^8.20.0",
        "vite": "^6.0.0",
        "vitest": "^3.0.0"
    },
    "dependencies": {
        "@fluidframework/azure-client": "^2.30.0",
        "@fluidframework/fluid-static": "^2.30.0",
        "@fluidframework/sequence": "^2.30.0",
        "@fluidframework/tree": "^2.30.0",
        "@inlang/paraglide-sveltekit": "^0.15.5",
        "dotenv": "^16.4.7",
        "firebase": "^11.5.0",
        "fluid-framework": "2.30.0",
        "pino": "^8.15.0",
        "uuid": "^8.3.2"
    }
}
