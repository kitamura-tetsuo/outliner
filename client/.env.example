# 環境設定
VITE_IS_TEST=false
VITE_IS_TEST_MODE_FORCE_E2E=false

# Local network host used by development scripts
LOCAL_HOST=localhost

# サーバー設定
VITE_HOST=localhost
# クラウド環境では 7070、エミュレータ利用時は 7090
VITE_PORT=7090
TEST_PORT=7070

# Firebase設定
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Azure Fluid Relay設定
VITE_AZURE_TENANT_ID=your-tenant-id
VITE_AZURE_FLUID_RELAY_ENDPOINT=https://us.fluidrelay.azure.com

# 接続サービスの選択
# true: Tinyliciousを使用、false: Azureを使用
VITE_USE_TINYLICIOUS=false
# 開発環境でAzureを強制的に使用する
VITE_FORCE_AZURE=true

# 認証設定
VITE_USE_FIREBASE_AUTH=true
VITE_USE_API_AUTH=true

# API設定
VITE_API_BASE_URL=https://your-project.web.app
VITE_API_SERVER_URL=https://your-project.web.app
VITE_AZURE_FUNCTION_URL=https://your-project.web.app
VITE_FIREBASE_FUNCTIONS_URL=https://your-project.web.app
VITE_FIREBASE_FUNCTIONS_HOST=localhost
VITE_FIREBASE_FUNCTIONS_PORT=57070
VITE_TOKEN_VERIFY_URL=http://localhost:3000/verify

# Tinylicious設定
VITE_TINYLICIOUS_HOST=localhost
VITE_TINYLICIOUS_ENDPOINT=http://localhost:7092
VITE_TINYLICIOUS_PORT=7092
VITE_FLUID_TOKEN_KEY=your-token-key

# Firebase Emulator設定（テスト環境用）
VITE_USE_FIREBASE_EMULATOR=false
VITE_FIRESTORE_EMULATOR_HOST=localhost
VITE_FIRESTORE_EMULATOR_PORT=58080
VITE_FIREBASE_AUTH_EMULATOR_HOST=localhost
VITE_AUTH_EMULATOR_PORT=59099
VITE_FIREBASE_EMULATOR_UI_PORT=54000

# デバッグ/テスト用設定
VITE_DEBUG_USER_ID=your-debug-user-id
VITE_DEBUG_USER_NAME=Your Debug User
VITE_DEBUG_USER_EMAIL=debug@example.com

# Fluid Framework Telemetry設定
VITE_DISABLE_FLUID_TELEMETRY=true
